{
  "server": {
    "hostname": "0.0.0.0",
    "publicFhirBase": "http://localhost:3447/fhir",
    "port": 3447,
    "fhirVersion": "dstu2"
  },
  "mongodb": {
    "url": "mongodb://localhost/hearth"
  },
  "authentication": {
    "authWindowSeconds": 10,
    "type": "openhim-style",
    "enablePublicUserCreation": false,
    "secret": null,
    "jwt": {
      "algorithm": "HS256",
      "secret": null,
      "privKey": null,
      "pubKey": null,
      "issuer": "hearth",
      "setAudience": "hearth/example-app1",
      "validateAudience": "^hearth:example-app\\d+$",
      "expiresIn": "1d"
    }
  },
  "logger": {
    "level": "info"
  },
  "idGenerator": "uuidv1",
  "atnaAudit": {
    "enabled": true,
    "interface": "udp",
    "host": "localhost",
    "port": 5050,
    "certOptions": {
      "key": null,
      "cert": null,
      "ca": null
    }
  },
  "matchingQueue": {
    "numberOfWorkers": 2,
    "pollingInterval": 1000
  },
  "validation": {
    "enabled": false,
    "additionProfilePaths": [
      "lib/fhir/profiles/mhd"
    ]
  },
  "knownIdentityDomains": []
}
